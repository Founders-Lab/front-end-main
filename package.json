    "@walletconnect/web3-provider": "^1.7.8",
    "clsx": "^1.1.1",
    "ethers": "^5.6.7",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "react": "^18.0.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.2.0",
    "react-loader-spinner": "^5.1.5",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.3.2",
    "redux": "^4.2.0",
    "sass": "^1.52.1",
    "vite-plugin-svgr": "^2.1.0",
