{
  "name": "frontend-main-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "tsc-check": "tsc",
    "tsc-watch": "tsc -w",
    "eslint": "esw src/**",
    "eslint-watch": "esw -w --changed src/**",
    "eslint-watch-quiet": "esw -w --quiet --changed src/**",
