    "@uauth/js": "^2.4.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "clsx": "^1.1.1",
    "ethers": "^5.6.7",
    "gsap": "^3.11.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lottie-react": "^2.3.1",
    "path": "^0.12.7",
    "react": "^18.0.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^1.3.0",
